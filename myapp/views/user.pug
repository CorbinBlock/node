    div.NavButtons
      a(href="//unix-01-debian:8080/job/prod/view/All/builds")
        div.NavButton Jenkins 
    div.NavButtons
      a(href="//github.com/CorbinBlock")
        div.NavButton Github 
    div.NavButtons
      a(href="/logout")
        div.NavButton Log out